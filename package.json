{
  "name": "webagent-platform",
  "version": "2.6.4",
  "description": "WebAgent Platform with improved test reliability, fixed AsyncTestCase issues, and ModelManager optimizations (2025-03-14)",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "diagnostics": "conda run -n agents python -m backend.diagnostics.runners.run_diagnostics",
    "tests": "conda run -n agents python -m backend.diagnostics.runners.run_tests",
    "check": "conda run -n agents python -m backend.diagnostics.runners.run_all",
    "research": "node scripts/run_webagent.js --mode workflow"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "ai": "^2.2.31",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "framer-motion": "^10.17.9",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-textarea-autosize": "^8.5.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
